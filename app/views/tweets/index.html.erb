<div class='container'>
    <h2>Search Results</h2>
    <br>
    <%= link_to  "Search again", search_path %>
    <div id='tweets'>
        <ul>
            <% if !@tweets.nil? %>
                <% @tweets.each do |tweet| %>
                    <li class='tweet'><%= tweet.text %> | <%= tweet.created_at %></li>
                <% end %>
            <% end %>
        </ul>
    </div>
</div>